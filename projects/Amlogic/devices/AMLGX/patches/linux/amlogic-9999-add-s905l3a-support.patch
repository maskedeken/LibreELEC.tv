+/dts-v1/;
+
+#include "meson-g12a-u200.dts"
+
+/ {
+	compatible = "m401a", "amlogic,g12a";
+	model = "M401A";
+	
+	aliases {
+		serial1 = &uart_A;
+	};
+	
