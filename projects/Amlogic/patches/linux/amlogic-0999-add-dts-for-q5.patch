diff -urN a/arch/arm64/boot/dts/amlogic/meson-gxbb-digital-video-q5.dts b/arch/arm64/boot/dts/amlogic/meson-gxbb-digital-video-q5.dts
--- a/arch/arm64/boot/dts/amlogic/meson-gxbb-digital-video-q5.dts	1970-01-01 00:00:00.000000000 +0000
+++ b/arch/arm64/boot/dts/amlogic/meson-gxbb-digital-video-q5.dts	2021-11-04 03:39:02.851820093 +0000
@@ -0,0 +1,29 @@
+#include "meson-gxbb-p200.dts"
+
+&ethmac {
+	status = "okay";
+	pinctrl-0 = <&eth_rgmii_pins>;
+	pinctrl-names = "default";
+	phy-handle = <&eth_phy0>;
+	phy-mode = "rgmii";
+
+	amlogic,tx-delay-ns = <2>;
+
+	mdio {
+		compatible = "snps,dwmac-mdio";
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+                eth_phy0: ethernet-phy@0 {
+			reg = <0>;
+
+			reset-assert-us = <10000>;
+			reset-deassert-us = <30000>;
+			reset-gpios = <&gpio GPIOZ_14 GPIO_ACTIVE_LOW>;
+
+			interrupt-parent = <&gpio_intc>;
+			/* MAC_INTR on GPIOZ_15 */
+			interrupts = <29 IRQ_TYPE_LEVEL_LOW>;
+		};
+	};
+};
